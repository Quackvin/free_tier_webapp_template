{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "platform": {
        "apiRuntime": "python:3.11"
    },
    "routes": [
        {
            "route": "/*",
            "allowedRoles": [
                "approved_user"
            ]
        }
    ],
    "responseOverrides": {
        "401": {
            "redirect": "/.auth/login/aad",
            "statusCode": 302
        }
    }
}